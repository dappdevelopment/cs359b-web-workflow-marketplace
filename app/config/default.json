{
  "host": "localhost",
  "port": 3232,
  "public": "../client/resources/public/",
  "contracts_dir": "../../build/contracts/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "remotes": {
    "automationServer": {
      "baseUri": "http://localhost:1337",
      "json": true
    }
  },
  "mongodb": "mongodb://localhost:27017/wflow_mkt",
  "authentication": {
    "secret": "",
    "strategies": [
      "jwt",
      "local"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "type": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "service": "users",
      "usernameField": "email",
      "passwordField": "password"
    }
  }
}
